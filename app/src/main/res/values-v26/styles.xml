<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="user_item_details_text_style" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:textColor">@color/grey</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">11sp</item>
        <item name="android:layout_marginLeft">10dp</item>
    </style>
    <style name="user_item_details_text_alt_style" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="android:fontFamily">sans-serif-thin</item>
        <item name="android:textColor">@color/grey</item>
        <item name="android:textSize">11sp</item>
        <item name="android:layout_marginLeft">10dp</item>
    </style>
    <style name="Button.White" parent="ThemeOverlay.AppCompat">
        <item name="colorAccent">@android:color/white</item>
    </style>
    <style name="AlertDialogCustom" parent="@android:style/Theme.Dialog">
        <item name="layout_constraintWidth_percent">100</item>
        <item name="android:windowMinWidthMajor">100%</item>
        <item name="android:windowMinWidthMinor">100%</item>
        <item name="android:textColor">#00FF00</item>
        <item name="android:layout_marginStart">4dp</item>
        <item name="android:layout_marginEnd">4dp</item>
        <item name="android:typeface">monospace</item>
        <item name="android:textSize">10sp</item>
    </style>
    <style name="AlertDialogCustom2" parent="@android:style/Theme.Dialog">
        <item name="layout_constraintWidth_percent">93</item>
        <item name="android:windowMinWidthMajor">93%</item>
        <item name="android:windowMinWidthMinor">93%</item>
        <item name="android:textColor">#00FF00</item>
        <item name="android:typeface">monospace</item>
        <item name="android:textSize">10sp</item>
    </style>
</resources>